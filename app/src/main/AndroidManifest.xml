<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.edu.usa.reto3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/chaquetaicono"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/chaquetaicono_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Reto2">
        <activity
            android:name="co.edu.usa.reto3.SplashScreen"
            android:exported="true"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name="co.edu.usa.reto3.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Reto2.NoActionBar"
            >

        </activity>
    </application>

</manifest>